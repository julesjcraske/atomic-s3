{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": [2, {"args": "all", "argsIgnorePattern": "^_"}],
    "comma-dangle": [1, "always-multiline"],
    "arrow-parens": [2, "as-needed", { "requireForBlockBody": false }],
    "no-unused-expressions": [0],
    "no-plusplus": [0],
    "max-len": [0]
  }
}
